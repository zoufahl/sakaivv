	{
  "name" : "Sakai KVV",
  "version" : "0.3",
  "description" : "Revert current lists of courses to old kvv lists",
  "browser_action": {
     "default_icon": "icon.png",
     "default_title": "Make kvv"
  },
  "permissions": [
    "activeTab", 
    "tabs"
  ],
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "content_scripts": [
    {
      "matches": ["http://*/*", "https://*/*"],
      "css": ["sakaivv.css"],
      "js": ["jquery.js"]
    }
  ],
  "manifest_version": 2
}
